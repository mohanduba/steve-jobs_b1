
public class ForTest {
public static void main(String[]args) {
int[]input= {10,20,30,40,50};
for(int i=0;i<input.length;i++) {
	if(i==input.length-1) {
		System.out.print(input[i]);
	}
	else
	System.out.print(input[i]+" , ");
	System.out.print(" ");
}

System.out.println(" ");
for(int val:input) {
	System.out.print(val+" , ");
	
}

}

}
